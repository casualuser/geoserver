<?xml version="1.0" encoding="UTF-8"?>
<resources>
	<resource class="vectorialLayer">
		<workspace>it.geosolutions</workspace>
		<name>OAATracks</name>
		<persistent>true</persistent>
		<defaultStyle>
			<name>point</name>
			<filename>point.sld</filename>
		</defaultStyle>
		<title>Optimal Asset Allocator tracks</title>
		<abstract>Optimal Asset Allocator tracks</abstract>
		<keywords>
			<string>Optimal Asset Allocator</string>
			<string>OAA</string>
			<string>navigation</string>
			<string>tracks</string>
		</keywords>
		<nativeCRS>GEOGCS["GCS_WGS_1984",
			DATUM["WGS_1984",
			SPHEROID["WGS_1984", 6378137.0, 298.257223563]],
			PRIMEM["Greenwich",
			0.0],
			UNIT["degree", 0.017453292519943295],
			AXIS["Longitude", EAST],
			AXIS["Latitude", NORTH]]
		</nativeCRS>
		<srs>EPSG:4326</srs>
		<!-- nativeBoundingBox> <minx>6.346</minx> <maxx>20.83</maxx> <miny>36.492</miny> 
			<maxy>46.591</maxy> <crs>EPSG:4326</crs> </nativeBoundingBox -->
		<latLonBoundingBox>
			<minx>6.346</minx>
			<maxx>20.83</maxx>
			<miny>36.492</miny>
			<maxy>46.591</maxy>
			<crs>EPSG:4326</crs>
		</latLonBoundingBox>
		<metadata>
			<cacheAgeMax>3600</cacheAgeMax>
			<kml.regionateFeatureLimit>10</kml.regionateFeatureLimit>
			<indexingEnabled>false</indexingEnabled>
			<cachingEnabled>true</cachingEnabled>
			<dirName>states</dirName>
		</metadata>
		<dimensions>
			<dimension>
				<name>time</name>
				<description />
				<min>2014-08-01T00:00:00Z</min>
				<max>2014-09-01T00:00:00Z</max>
				<dimensionInfo>
					<enabled>true</enabled>
					<attribute>Time</attribute>
					<presentation>DISCRETE_INTERVAL</presentation>
					<resolution>1800000</resolution>
					<unitSymbol />
					<units>ISO8601</units>
					<defaultValue />
				</dimensionInfo>
			</dimension>
		</dimensions>
		<translateContext>
			<item class="dataStore" order="0">
				<store>
					<url>file:tracks_filtered.csv</url>
				</store>
			</item>
			<item class="transform" order="1"
				type="org.geoserver.importer.transform.AttributesToPointGeometryTransform">
				<transform>
					<latField>Lat</latField>
					<lngField>Lon</lngField>
					<pointFieldName>location</pointFieldName>
					<geometryFactory>
						<coordinateSequenceFactory
							class="com.vividsolutions.jts.geom.impl.CoordinateArraySequenceFactory" />
						<SRID>4326</SRID>
					</geometryFactory>
				</transform>
			</item>
			<item class="transform" order="2"
				type="org.geoserver.importer.transform.EpochToDateTransform">
				<transform>
					<field>Time</field>
					<epochMux>1000</epochMux>
				</transform>
			</item>
			<item class="dataStore" order="3">
				<store>
					<dbtype>postgis</dbtype>
					<host>localhost</host>
					<port>5432</port>
					<schema>geoserver</schema>
					<database>geoserver</database>
					<user>geoserver</user>
					<passwd>geoserver</passwd>
					<!-- additional params: the ___ will become a space -->
					<Connection___timeout>20</Connection___timeout>
					<encode___functions>false</encode___functions>
					<Evictor___run___periodicity>300</Evictor___run___periodicity>
					<namespace>http://www.cmre.nato.int/remotewps_oaa</namespace>
					<create___database>false</create___database>
					<fetch___size>1000</fetch___size>
					<preparedStatements>false</preparedStatements>
					<min___connections>1</min___connections>
					<Evictor___tests___per___run>3</Evictor___tests___per___run>
					<validate___connections>true</validate___connections>
					<max___connections>10</max___connections>
					<Support___on___the___fly___geometry___simplification>true
					</Support___on___the___fly___geometry___simplification>
					<Max___connection___idle___time>300
					</Max___connection___idle___time>
					<Test___while___idle>true</Test___while___idle>
					<Loose___bbox>true</Loose___bbox>
					<Expose___primary___keys>false</Expose___primary___keys>
					<Max___open___prepared___statements>50
					</Max___open___prepared___statements>
					<Estimated___extends>true</Estimated___extends>
				</store>
			</item>
		</translateContext>
	</resource>
