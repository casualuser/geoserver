<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">
<beans>

	<bean id="resourceLoaderProcess" class="org.geoserver.wps.gs.resource.ResourceLoaderProcess">
		<constructor-arg index="0" ref="geoServer" />
		<constructor-arg index="1" ref="wpsResourceManager" />
	</bean>

	<!-- ResourceLoader Pluggable Converters -->
	<bean id="vectorialLayerResourceConverter"
		class="org.geoserver.wps.gs.resource.model.plugin.VectorialLayerConverter">
		<constructor-arg index="0" value="vectorialLayer" />
	</bean>
</beans>