<?xml version="1.0" encoding="UTF-8"?>
<as:AppSchemaDataAccess xmlns:as="http://www.geotools.org/app-schema"
                        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                        xsi:schemaLocation="http://www.geotools.org/app-schema AppSchemaDataAccess.xsd">
  <namespaces>
    <Namespace>
      <prefix>st_gml31</prefix>
      <uri>http://www.stations_gml31.org/1.0</uri>
    </Namespace>
    <Namespace>
      <prefix>ms_gml31</prefix>
      <uri>http://www.measurements_gml31.org/1.0</uri>
    </Namespace>
  </namespaces>
  <sourceDataStores>
    <DataStore>
      <id>stations_gml31</id>
      <parameters>
        <Parameter>
          <name>directory</name>
          <value>file:./</value>
        </Parameter>
      </parameters>
    </DataStore>
  </sourceDataStores>
  <targetTypes>
    <FeatureType>
      <schemaUri>stations_gml31.xsd</schemaUri>
    </FeatureType>
  </targetTypes>
  <typeMappings>
    <FeatureTypeMapping>
      <sourceDataStore>stations_gml31</sourceDataStore>
      <sourceType>stations_gml31</sourceType>
      <targetElement>st_gml31:Station_gml31</targetElement>
      <mappingName>isolated_1_station_gml31</mappingName>
      <attributeMappings>
        <AttributeMapping>
          <targetAttribute>st_gml31:Station_gml31</targetAttribute>
          <idExpression>
            <OCQL>ID</OCQL>
          </idExpression>
        </AttributeMapping>
        <AttributeMapping>
          <targetAttribute>st_gml31:name</targetAttribute>
          <sourceExpression>
            <OCQL>strConCat('isolated_1_', NAME)</OCQL>
          </sourceExpression>
        </AttributeMapping>
        <AttributeMapping>
          <targetAttribute>st_gml31:location</targetAttribute>
          <sourceExpression>
            <OCQL>LOCATION</OCQL>
          </sourceExpression>
        </AttributeMapping>
        <AttributeMapping>
          <targetAttribute>st_gml31:measurements</targetAttribute>
          <sourceExpression>
            <OCQL>ID</OCQL>
            <linkElement>isolated_1_measurement_gml31</linkElement>
            <linkField>FEATURE_LINK[1]</linkField>
          </sourceExpression>
        </AttributeMapping>
      </attributeMappings>
    </FeatureTypeMapping>
  </typeMappings>
</as:AppSchemaDataAccess>
